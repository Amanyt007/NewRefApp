@model IEnumerable<NewRefApp.Models.UpiDetails>

@{
    ViewData["Title"] = "UPI Details";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>UPI Details</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>UPI ID</th>
            <th>Is Admin</th>
            <th>Status</th>
            <th>Created Date</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.UpiId</td>
                <td>@item.IsAdmin</td>
                <td>@item.Status</td>
                <td>@item.CreatedDate</td>
                <td>
                    <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                    <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                    <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>